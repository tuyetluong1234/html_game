mixin body
  body(class!=attributes.class)
    block body-contents

    = "\n"
    // Vendor jQuery (CORE PLUGINS - METRONIC)
    script(type='text/javascript', src='../../libs/metronic-admin/global/plugins/jquery.min.js')
    script(type='text/javascript', src='../../libs/metronic-admin/global/plugins/jquery-migrate.min.js')
    script(type='text/javascript', src='../../libs/metronic-admin/global/plugins/bootstrap/js/bootstrap.min.js')
    script(type='text/javascript', src='../../libs/metronic-admin/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js')
    script(type='text/javascript', src='../../libs/metronic-admin/global/plugins/uniform/jquery.uniform.min.js')
    script(type='text/javascript', src='../../libs/metronic-admin/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js')
    
    ="\n"
    // Vendor jQuery (PAGE LEVEL PLUGINS - METRONIC)
    script(type='text/javascript', src='../../libs/metronic-admin/global/plugins/bootstrap-datepicker/js/bootstrap-datepicker.min.js')
    // IMPORTANT! fullcalendar depends on jquery-ui.min.js for drag & drop support, and moment.min.js
    script(type='text/javascript', src='../../libs/metronic-admin/global/plugins/moment.min.js')

    ="\n"
    // Vendor jQuery (PAGE LEVEL STYLES - METRONIC)
    script(type='text/javascript', src='../../libs/metronic-admin/global/scripts/metronic.js')
    script(type='text/javascript', src='../../libs/metronic-admin/admin/layout/scripts/layout.js')
    script(type='text/javascript', src='../../libs/metronic-admin/admin/layout/scripts/demo.js')

    // hight chart
    script(type='text/javascript', src='https://code.highcharts.com/highcharts.js')
    script(type='text/javascript', src='https://code.highcharts.com/modules/series-label.js')
    script(type='text/javascript', src='https://code.highcharts.com/modules/exporting.js')
    script(type='text/javascript', src='https://code.highcharts.com/modules/export-data.js')

    block js
      // Gentelella Custom jQuery
      script(src='../../libs/custom.js')
      = "\n"
      // Theme Script
      script(type="text/javascript", src="../../assets/js/bootstrap-select.min.js")
      //- script(type="text/javascript", src="../../assets/js/sidebar-menu.js")
      script(type="text/javascript", src="../../assets/js/admin.js")
      script(type="text/javascript", src="../../assets/js/customer.js")
    
    
