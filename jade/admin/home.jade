                          img( src="../../assets/images/circle.png" alt="menu")
                      .ts-background(data-bg-parallax="scroll" data-bg-parallax-speed="3")
                        .ts-background-image.ts-parallax-element(data-bg-image="../../assets/images/bgr_nv.png" data-animate="ts-zoomOutIn")
