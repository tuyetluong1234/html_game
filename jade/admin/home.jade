        .container
          include ../includes/page-sidebar-left.jade
          .content-wapper-page
            .content-page
              .cnt-hero-switch
                .list-switch
                  .item-switch
                    .text Your Heroes
                    .input-check
                      input#switch1(type="checkbox")
                      label(for="switch1")
                  .item-switch
                    .text Your Offers
                    .input-check
                      input#switch2(type="checkbox")
                      label(for="switch2")
