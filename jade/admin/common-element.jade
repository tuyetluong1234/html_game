extends ../layouts/master.jade

append titlePage
  title Common Element - RestWalk

append body
  +body(class="")

append css
  style.
    body {
      background: #E8E8E8;
    }
    #example-wrapper {
      padding-top: 40px;
      margin: auto;
    }
    #example-wrapper .well {
      background: #fff;
      border-radius: 4px;
      padding: 10px;
      margin-bottom: 20px;
    }
    #example-wrapper .well:before,
    #example-wrapper .well:after {
      content: " ";
      clear: both;
      display: table;
    }
    #example-wrapper .t1 {
      font-size: 30px;
      font-weight: bold;
      text-transform: uppercase;
      margin-top: 80px;
      margin-bottom: 10px;
    }
    #example-wrapper .t2 {
      font-size: 16px;
      font-weight: bold;
      margin-bottom: 10px;
    }
    #example-wrapper .rw-inputbox-wrapper {
      max-width: 300px;
    }
    #example-wrapper .bg1 {
      background-color: #ff7831 !important;
    }
    #example-wrapper .bg2 {
      background-color: #f5f5f5 !important;
    }

append body-contents
  #example-wrapper
    .container
      .t1 Typo
      
      .t2 .rw-f{x}
