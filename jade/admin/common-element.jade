                  span.help-block
                    | select a date
              .form-group
                label.control-label.col-md-3
                  | Membership 
                  span.required *
                .col-md-4
                  .radio-list(data-error-container='#form_2_membership_error')
                    label
                      input(type='radio', name='membership', value='1')
                      |                         Fee
                    label
                      input(type='radio', name='membership', value='2')
                      |                         Professional
                  #form_2_membership_error
              .form-group
                label.control-label.col-md-3
                  | Services 
                  span.required *
                .col-md-4
                  .checkbox-list.checkall(data-error-container='#form_2_services_error')
                    input#all(type='checkbox', value='1', name='all')
                    span check all list checkbox
                    label
