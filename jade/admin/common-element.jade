                      |  Service 1
                    label
                      input(type='checkbox', value='2', name='service')
                      |  Service 2
                    label
                      input(type='checkbox', value='3', name='service')
                      |  Service 3
                  span.help-block
                    | (select at least two)
                  #form_2_services_error
              .form-group.last
                label.control-label.col-md-3
                  | CKEditor 
                  span.required *
                .col-md-9
                  textarea.rw-ckeditor.form-control(name='editor2', rows='6', data-error-container='#editor2_error')
                  #editor2_error
            .form-actions
              .row
                .col-md-offset-3.col-md-9
                  button.btn.green(type='submit') Submit
                  button.btn.default(type='button') Cancel
            // input upload img
            .form-actions
              .row
                .col-md-offset-3.col-md-9
                  span input upload file
                  .inputfile
                    .tr-file-upload.image-upload
